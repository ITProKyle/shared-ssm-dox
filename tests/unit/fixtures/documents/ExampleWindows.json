{
    "schemaVersion": "2.2",
    "description": "Example document.",
    "parameters": {
        "Name": {
            "default": "user",
            "type": "String"
        }
    },
    "mainSteps": [
        {
            "action": "aws:runPowerShellScript",
            "inputs": {
                "runCommand": [
                    "$Name = {{Name}}",
                    "Write-Output $Name"
                ]
            },
            "name": "LinuxGreetings",
            "precondition": {
                "StringEquals": [
                    "platformType",
                    "Windows"
                ]
            }
        }
    ]
}
